    "## Venu Thatikonda\n",
    "\n",
    "## arrange a given string in alphabetical order\n",
    "\n",
    "def alphabet_soup(my_str):\n",
    "\n",
    "    res = ''.join(sorted(my_str))\n",
    "    \n",
    "    print(res)\n",
    "        "
   ]
