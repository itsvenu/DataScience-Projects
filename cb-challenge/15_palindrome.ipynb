    "## True if given string is palindrome\n",
    "\n",
    "def check_palindrom(my_str):\n",
    "    \n",
    "    my_str_rev = my_str[::-1]\n",
    "    \n",
    "    if my_str == my_str_rev:\n",
    "        print(True)\n",
    "    else:\n",
    "        print(False)"
